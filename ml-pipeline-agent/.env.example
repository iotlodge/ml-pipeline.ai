# Environment
ENV=dev
LOG_LEVEL=INFO

# LLM Providers
LLM_PROVIDER=anthropic
ANTHROPIC_API_KEY=sk-ant-xxxxx
OPENAI_API_KEY=sk-xxxxx
LLM_MODEL_CLAUDE=claude-sonnet-4-5-20250929
LLM_MODEL_OPENAI=gpt-4o

# Execution Sandbox
SANDBOX_TYPE=subprocess
SANDBOX_TIMEOUT_SEC=60
SANDBOX_MEMORY_LIMIT_MB=512

# Pipeline
MAX_LOOPS=3
CHECKPOINT_ENABLED=true
CHECKPOINT_BACKEND=sqlite

# Storage
ARTIFACT_STORAGE=local
ARTIFACT_LOCAL_PATH=/tmp/ml-pipeline/artifacts
ARTIFACT_S3_BUCKET=
AWS_REGION=us-east-1

# API
API_HOST=0.0.0.0
API_PORT=8000

# Observability
LANGSMITH_ENABLED=false
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=ml-pipeline-agent
